          {{#each}}
            <li id="image">{{image}}</li>
            <li id="name">Name: {{name}}</li>
            <li id="location">Location: {{location}}</li>
            <li id="contact">Contact: {{contact}}</li>
            <li id="about">About: {{about}}</li>
          {{/each}}
        </ul>
      </div>
    </div>
