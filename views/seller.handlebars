    <div class="row">
      <div class="col">
        <div class="card" style="width: 18rem;">
          {{#each}}
            <img src="..." class="card-img-top" alt="..." id="picture">

            <ul class="list-group list-group-flush">
              <li class="list-group-item" id="item-name">Item: {{item-name}}</li>
              <li class="list-group-item" id="price">Price: {{price}}</li>
              <li class="list-group-item" id="quantity">Quantity: {{quantity}}</li>
              <li class="list-group-item" id="status">Status: {{status}}</li>
              <li class="list-group-item" id="description">Description: {{description}}</li>
            </ul>
          {{/each}}
            <div class="card-body">
              <a href="#" class="card-link" type="submit">Save</a>
              <a href="#" class="card-link" type="submit">Edit</a>
              <a href="#" class="card-link" type="submit">Delete</a>
